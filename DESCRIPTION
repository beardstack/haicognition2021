Package: haicognition2021
Title: Reproducible research materials for HAI and cognition study
Version: 0.0.0.9000
Authors@R: 
    person(given = "Jeffrey R.",
           family = "Stevens",
           role = c("aut", "cre"),
           email = "jeffrey.r.stevens@gmail.com",
